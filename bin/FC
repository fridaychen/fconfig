#!/bin/bash

function fc-include() {
    for x in $*; do
        [[ -f $x ]] && source "${x}"
    done
}

fc-include \
    ${FCHOME}/bin/Fcommon.sh \
    ${FCHOME}/bin/Fansi.sh \
    ${FCHOME}/bin/Falias.sh \
    ${FCHOME}/bin/Fproject.sh \
    ${FCHOME}/bin/Fmedia.sh \
    ${FCHOME}/bin/Fgit.sh \
    ${FCHOME}/bin/Frepo.sh \
    ${FCHOME}/bin/Finit.sh \
    ${FCHOME}/bin/Fps.sh

${FCHOME}/bin/Fwelcome.sh
